dex.js",[],"C:\\dev\\chatbot\\src\\components\\action-provider\\action-provider.js",["64"],"import React from 'react';\r\n\r\nclass ActionProvider {\r\n    constructor(createChatBotMessage, setStateFunc, createClientMessage) {\r\n        this.createChatBotMessage = createChatBotMessage;\r\n        this.setState = setStateFunc;\r\n        this.createClientMessage = createClientMessage;\r\n    }\r\n\r\n    updateChatbotState = (message) => {\r\n        // NOTE: This function is set in the constructor, and is passed in      \r\n        // from the top level Chatbot component. The setState function here     \r\n        // actually manipulates the top level state of the Chatbot, so it's     \r\n        // important that we make sure that we preserve the previous state.\r\n\r\n        this.setState(prevState => ({\r\n            ...prevState,\r\n            messages: [...prevState.messages, message]\r\n        }));\r\n    }      \r\n\r\n    handleDefaultOptions = () => {\r\n        const message = this.createChatBotMessage(\r\n            '–í—ã–±–µ—Ä–∏—Ç–µ –∏–Ω—Ç–µ—Ä–µ—Å—É—é—â–∏–π —Ä–∞–∑–¥–µ–ª, —á—Ç–æ–±—ã —è —Å–º–æ–≥ –≤–∞–º –ø–æ–º–æ—á—åüòä',\r\n            {\r\n                widget: 'defaultOptions'\r\n            }\r\n        );\r\n\r\n        this.updateChatbotState(message);\r\n    }\r\n\r\n    handleQuestion = (responseText, widgetName) => {\r\n        const message = this.createChatBotMessage( responseText, {widget: widgetName});\r\n\r\n        this.updateChatbotState(message);\r\n    }\r\n\r\n\r\n\r\n    // START USAGE SECTION\r\n\r\n    handleStartUsageQuestionList = () => { \r\n        const responseText = '–í–æ–ø—Ä–æ—Å—ã –ø–æ —Ç–µ–º–µ \"–ù–∞—á–∞–ª–æ —Ä–∞–±–æ—Ç—ã —Å –ø–æ—Ä—Ç–∞–ª–æ–º\"';\r\n        const widgetName = 'startUsageQuestionList';\r\n\r\n        this.handleQuestion(responseText, widgetName);\r\n    }\r\n\r\n    handleStartUsageQuestion = (id) => {\r\n        const responses = {\r\n            1: '–ß—Ç–æ–±—ã –Ω–∞—á–∞—Ç—å —Ä–∞–±–æ—Ç—É, –∑–∞—Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä—É–π—Ç–µ—Å—å –Ω–∞ –ø–æ—Ä—Ç–∞–ª–µ. –û–±—è–∑–∞—Ç–µ–ª—å–Ω–æ –æ–∑–Ω–∞–∫–æ–º—å—Ç–µ—Å—å —Å ¬´–†–µ–≥–ª–∞–º–µ–Ω—Ç–æ–º –ø—Ä–∏—ë–º–∞ –∏ –æ–±—Ä–∞–±–æ—Ç–∫–∏ —Å–æ–æ–±—â–µ–Ω–∏–π¬ª, –∫–æ—Ç–æ—Ä—ã–π –º–æ–∂–Ω–æ –Ω–∞–π—Ç–∏ –≤ –ø—É–Ω–∫—Ç–µ \"–û –ø—Ä–æ–µ–∫—Ç–µ\" –º–µ–Ω—é –ø–æ—Ä—Ç–∞–ª–∞.',\r\n            2: '–ê–≤—Ç–æ—Ä–∏–∑–æ–≤–∞–Ω–Ω—ã–º –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è–º –¥–æ—Å—Ç—É–ø–Ω—ã –≤—Å–µ –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç–∏ –ü–æ—Ä—Ç–∞–ª–∞, —Ç–∞–∫–∏–µ –∫–∞–∫ –æ—Ç–ø—Ä–∞–≤–∫–∞ —Å–æ–æ–±—â–µ–Ω–∏–π –æ –ø—Ä–æ–±–ª–µ–º–∞—Ö, —É—á–∞—Å—Ç–∏–µ –≤ –≥–æ–ª–æ—Å–æ–≤–∞–Ω–∏—è—Ö, –ø—Ä–µ–¥–ª–æ–∂–µ–Ω–∏–µ –∏–Ω–∏—Ü–∏–∞—Ç–∏–≤ –∏ –æ–¥–æ–±—Ä–µ–Ω–∏–µ –ø—Ä–æ–µ–∫—Ç–æ–≤ –ø–æ –±–ª–∞–≥–æ—É—Å—Ç—Ä–æ–π—Å—Ç–≤—É. –≠—Ç–æ –æ—Ç–ª–∏—á–Ω–∞—è –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç—å –ø–æ–≤–ª–∏—è—Ç—å –Ω–∞ —Ä–∞–∑–≤–∏—Ç–∏–µ —Å–≤–æ–µ–≥–æ –≥–æ—Ä–æ–¥–∞ –∏ —Ä–µ–≥–∏–æ–Ω–∞.',\r\n            3: '–ß—Ç–æ–±—ã –∑–∞—Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä–æ–≤–∞—Ç—å —É—á—ë—Ç–Ω—É—é –∑–∞–ø–∏—Å—å –Ω—É–∂–Ω–æ –Ω–∞–∂–∞—Ç—å –Ω–∞ –∫–Ω–æ–ø–∫—É ¬´–ó–∞—Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä–æ–≤–∞—Ç—å—Å—è¬ª –≤ –ø—Ä–∞–≤–æ–º –≤–µ—Ä—Ö–Ω–µ–º —É–≥–ª—É —ç–∫—Ä–∞–Ω–∞. –û—Ç–∫—Ä–æ–µ—Ç—Å—è —Ä–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏–æ–Ω–Ω–∞—è —Ñ–æ—Ä–º–∞. –î–∞–ª–µ–µ —Å–ª–µ–¥—É–µ—Ç –∑–∞–ø–æ–ª–Ω–∏—Ç—å –≤—Å–µ –ø–æ–ª—è —Ñ–æ—Ä–º—ã, —É–∫–∞–∑–∞–≤ —Å–≤–æ–∏ –Ω–∞—Å—Ç–æ—è—â–∏–µ –¥–∞–Ω–Ω—ã–µ. –ü—Ä–æ–≤–µ—Ä—å—Ç–µ –ø—Ä–∞–≤–∏–ª—å–Ω–æ—Å—Ç—å –∑–∞–ø–æ–ª–Ω–µ–Ω–∏—è —Ñ–æ—Ä–º—ã. –ù–∞–∂–º–∏—Ç–µ –∫–Ω–æ–ø–∫—É ¬´–†–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏—è¬ª. –ü–æ—Å–ª–µ —ç—Ç–æ –≤—ã –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –∞–≤—Ç–æ—Ä–∏–∑—É–µ—Ç–µ—Å—å –∏ –±—É–¥–µ—Ç–µ –ø–µ—Ä–µ–Ω–∞–ø—Ä–∞–≤–ª–µ–Ω—ã –Ω–∞ –≥–ª–∞–≤–Ω—É—é —Å—Ç—Ä–∞–Ω–∏—Ü—É –ü–æ—Ä—Ç–∞–ª–∞. –î–∞–ª–µ–µ –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ –ø–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å –∞–¥—Ä–µ—Å —ç–ª–µ–∫—Ç—Ä–æ–Ω–Ω–æ–π –ø–æ—á—Ç—ã –≤ –ª–∏—á–Ω–æ–º –∫–∞–±–∏–Ω–µ—Ç–µ.',\r\n            4: '–ü—Ä–æ–≤–µ—Ä—å—Ç–µ, —á—Ç–æ –≤—ã –∑–∞–ø–æ–ª–Ω–∏–ª–∏ –≤—Å–µ –ø–æ–ª—è —Ñ–æ—Ä–º—ã —Ä–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏–∏.',\r\n        };\r\n        \r\n        const responseText = responses[id];\r\n        const widgetName = 'startUsageConversationReturn';\r\n\r\n        this.handleQuestion(responseText, widgetName);\r\n    }\r\n    \r\n    // HANDLE PROFILE QUESTIONS\r\n\r\n    handleProfileQuestionList = () => {\r\n        const responseText = '–í–æ–ø—Ä–æ—Å—ã –ø–æ —Ç–µ–º–µ \"–õ–∏—á–Ω—ã–π –∫–∞–±–∏–Ω–µ—Ç\"';\r\n        const widgetName = 'profileQuestionList';\r\n\r\n        this.handleQuestion(responseText, widgetName);\r\n    }\r\n\r\n    handleProfileQuestion = (id) => {\r\n        const responses = {\r\n            1: '–ß—Ç–æ–±—ã –≤–æ–π—Ç–∏ –≤ —Å–≤–æ—é —É—á—ë—Ç–Ω—É—é –∑–∞–ø–∏—Å—å, –≤–∞–º –Ω—É–∂–Ω–æ –Ω–∞–π—Ç–∏ –±–µ–ª—É—é –∫–Ω–æ–ø–∫—É \"–í–æ–π—Ç–∏\" –≤ –ø—Ä–∞–≤–æ–º –≤–µ—Ä—Ö–Ω–µ–º —É–≥–ª—É –ü–æ—Ä—Ç–∞–ª–∞, –∑–∞—Ç–µ–º –Ω–∞–∂–∞—Ç—å –Ω–∞ –Ω–µ—ë, –≤–≤–µ—Å—Ç–∏ —Å–≤–æ–∏ —É—á—ë—Ç–Ω—ã–µ –¥–∞–Ω–Ω—ã–µ –∏ –Ω–∞–∂–∞—Ç—å –∫—Ä–∞—Å–Ω—É—é –∫–Ω–æ–ø–∫—É \"–í–æ–π—Ç–∏\". –ï—Å–ª–∏ –≤—ã –Ω–µ –Ω–∞—à–ª–∏ –±–µ–ª—É—é –∫–Ω–æ–ø–∫—É \"–í–æ–π—Ç–∏\", –≤–æ–∑–º–æ–∂–Ω–æ –≤—ã —É–∂–µ –≤–æ—à–ª–∏ –≤ —Å–≤–æ—é —É—á—ë—Ç–Ω—É—é –∑–∞–ø–∏—Å—å.',\r\n            2: '–ß—Ç–æ–±—ã –∑–∞–π—Ç–∏ –≤ –õ–∏—á–Ω—ã–π –∫–∞–±–∏–Ω–µ—Ç –Ω—É–∂–Ω–æ –Ω–∞–∂–∞—Ç—å –Ω–∞ –∑–Ω–∞—á–æ–∫ —É—á–µ—Ç–Ω–æ–π –∑–∞–ø–∏—Å–∏ —Å –≤–∞—à–∏–º –∏–º–µ–Ω–µ–º –≤ –ø—Ä–∞–≤–æ–º –≤–µ—Ä—Ö–Ω–µ–º —É–≥–ª—É —ç–∫—Ä–∞–Ω–∞ –∏ –≤ –≤—ã–ø–∞–¥–∞—é—â–µ–º —Å–ø–∏—Å–∫–µ –≤—ã–±—Ä–∞—Ç—å –∏–Ω—Ç–µ—Ä–µ—Å—É—é—â–∏–π —Ä–∞–∑–¥–µ–ª. –ï—Å–ª–∏ –≤—ã –Ω–µ –Ω–∞—à–ª–∏ —Ç–∞–∫–æ–π –∑–Ω–∞—á–æ–∫, —É–±–µ–¥–∏—Ç–µ—Å—å, —á—Ç–æ –≤—ã –≤–æ—à–ª–∏ –≤ —Å–≤–æ—é —É—á—ë—Ç–Ω—É—é –∑–∞–ø–∏—Å—å.',\r\n            3: '–ü—Ä–æ–≤–µ—Ä—å—Ç–µ –ø–∞–ø–∫—É \"–°–ø–∞–º\" –≤ —Å–≤–æ–µ–π —ç–ª. –ø–æ—á—Ç–µ. –ï—Å–ª–∏ –≤—ã –Ω–µ –ø–æ–ª—É—á–∏–ª–∏ –ø–∏—Å—å–º–æ –¥–ª—è –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏—è —ç–ª–µ–∫—Ç—Ä–æ–Ω–Ω–æ–π –ø–æ—á—Ç—ã –Ω–∞ –ø–æ—Ä—Ç–∞–ª–µ, —Å–∫–æ—Ä–µ–µ –≤—Å–µ–≥–æ –≤—ã –Ω–µ–≤–µ—Ä–Ω–æ —É–∫–∞–∑–∞–ª–∏ –∞–¥—Ä–µ—Å —ç–ª–µ–∫—Ç—Ä–æ–Ω–Ω–æ–π –ø–æ—á—Ç—ã –ø—Ä–∏ —Ä–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏–∏. –ß—Ç–æ–±—ã –∑–∞—Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä–æ–≤–∞—Ç—å –∫–æ—Ä—Ä–µ–∫—Ç–Ω—ã–π –∞–¥—Ä–µ—Å —ç–ª–µ–∫—Ç—Ä–æ–Ω–Ω–æ–π –ø–æ—á—Ç—ã —Å–ª–µ–¥—É–µ—Ç –≤—ã–π—Ç–∏ –∏–∑ —Ç–µ–∫—É—â–µ–π —É—á–µ—Ç–Ω–æ–π –∑–∞–ø–∏—Å–∏, –Ω–∞–∂–∞–≤ –Ω–∞ –∑–Ω–∞—á–æ–∫ —É—á—ë—Ç–Ω–æ–π –∑–∞–ø–∏—Å–∏ –≤ –ø—Ä–∞–≤–æ–º –≤–µ—Ä—Ö–Ω–µ–º —É–≥–ª—É —ç–∫—Ä–∞–Ω–∞ –∏ –≤ –≤—ã–ø–∞–¥–∞—é—â–µ–º —Å–ø–∏—Å–∫–µ –≤—ã–±—Ä–∞—Ç—å –ø—É–Ω–∫—Ç ¬´–í—ã–π—Ç–∏¬ª. –ó–∞—Ç–µ–º –Ω–∞–∂–º–∏—Ç–µ –∫–Ω–æ–ø–∫—É ¬´–ó–∞—Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä–æ–≤–∞—Ç—å—Å—è¬ª –∏ –∑–∞–ø–æ–ª–Ω–∏—Ç–µ —Ñ–æ—Ä–º—É —Ä–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏–∏ –∑–∞–Ω–æ–≤–æ.',\r\n            4: `–û—Ç–∫—Ä–æ–π—Ç–µ —Ñ–æ—Ä–º—É –∞–≤—Ç–æ—Ä–∏–∑–∞—Ü–∏–∏ –ø–æ –∫–Ω–æ–ø–∫–µ ¬´–í–æ–π—Ç–∏¬ª –≤ –ø—Ä–∞–≤–æ–º –≤–µ—Ä—Ö–Ω–µ–º —É–≥–ª—É —ç–∫—Ä–∞–Ω–∞. –ü–µ—Ä–µ–π–¥–∏—Ç–µ –ø–æ —Å—Å—ã–ª–∫–µ ¬´–ó–∞–±—ã–ª–∏ –ø–∞—Ä–æ–ª—å?¬ª –Ω–∞ –æ—Ç–∫—Ä—ã–≤—à–µ–π—Å—è —Ñ–æ—Ä–º–µ –∞–≤—Ç–æ—Ä–∏–∑–∞—Ü–∏–∏. –ü—Ä–æ–∏–∑–æ–π–¥–µ—Ç –ø–µ—Ä–µ—Ö–æ–¥ –Ω–∞ —Ñ–æ—Ä–º—É –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è –ø–∞—Ä–æ–ª—è ¬´–°–±—Ä–æ—Å–∏—Ç—å –ø–∞—Ä–æ–ª—å¬ª. –í –ø–æ–ª–µ ¬´Email¬ª –≤–≤–µ–¥–∏—Ç–µ –∞–¥—Ä–µ—Å —ç–ª–µ–∫—Ç—Ä–æ–Ω–Ω–æ–π –ø–æ—á—Ç—ã, –Ω–∞ –∫–æ—Ç–æ—Ä—É—é –∑–∞—Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä–æ–≤–∞–Ω–∞ –≤–∞—à–∞ —É—á—ë—Ç–Ω–∞—è –∑–∞–ø–∏—Å—å. –ù–∞ –≤–∞—à—É –ø–æ—á—Ç—É –±—É–¥–µ—Ç –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ –æ —Å–±—Ä–æ—Å–µ –ø–∞—Ä–æ–ª—è. –í —ç—Ç–æ–º —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–∏ –ø–µ—Ä–µ–π–¥–∏—Ç–µ –ø–æ —Å—Å—ã–ª–∫–µ ¬´–°–±—Ä–æ—Å–∏—Ç—å –ø–∞—Ä–æ–ª—å¬ª. –í—ã –±—É–¥–µ—Ç–µ –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω—ã –Ω–∞ —Ñ–æ—Ä–º—É —Å–±—Ä–æ—Å–∞ –ø–∞—Ä–æ–ª—è, –≤ –∫–æ—Ç–æ—Ä–æ–π —Å–ª–µ–¥—É–µ—Ç —É–∫–∞–∑–∞—Ç—å –Ω–æ–≤—ã–π –ø–∞—Ä–æ–ª—å –∏ –ø–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç—å —Å–±—Ä–æ—Å.`,\r\n        };\r\n        \r\n        const responseText = responses[id];\r\n        const widgetName = 'profileConversationReturn';\r\n\r\n        this.handleQuestion(responseText, widgetName);\r\n    }    \r\n\r\n    // HANDLE COMPLAINTS QUESTIONS\r\n\r\n    handleComplaintsQuestionList = () => {\r\n        const responseText = '–í–æ–ø—Ä–æ—Å—ã –ø–æ —Ç–µ–º–µ \"–°–æ–æ–±—â–µ–Ω–∏—è\"';\r\n        const widgetName = 'complaintsQuestionList';\r\n\r\n        this.handleQuestion(responseText, widgetName); \r\n    }\r\n\r\n    handleComplaintsQuestion = (id) => {\r\n        const responses = {\r\n            1: '–°–æ–æ–±—â–∞—è –æ –Ω–µ–¥–æ—Å—Ç–∞—Ç–∫–∞—Ö –≤ —Ä–∞–±–æ—Ç–µ –∫–æ–º–º—É–Ω–∞–ª—å–Ω—ã—Ö –∏ –≥–æ—Ä–æ–¥—Å–∫–∏—Ö —Å–ª—É–∂–±, –ø—Ä–æ–±–ª–µ–º–∞—Ö –±–ª–∞–≥–æ—É—Å—Ç—Ä–æ–π—Å—Ç–≤–∞ –∏ –∫–∞—á–µ—Å—Ç–≤–∞ –∂–∏–∑–Ω–∏ –≤—ã –ø–æ–º–æ–≥–∞–µ—Ç–µ –æ—Ä–≥–∞–Ω–∞–º –∏—Å–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–æ–π –≤–ª–∞—Å—Ç–∏ –∫–æ–Ω—Ç—Ä–æ–ª–∏—Ä–æ–≤–∞—Ç—å –∏ —Å–≤–æ–µ–≤—Ä–µ–º–µ–Ω–Ω–æ —É—Å—Ç—Ä–∞–Ω—è—Ç—å –Ω–µ–¥–æ—á–µ—Ç—ã –ø–æ —Å–æ—Å—Ç–æ—è–Ω–∏—é –∏–Ω—Ñ—Ä–∞—Å—Ç—Ä—É–∫—Ç—É—Ä—ã, –ø—Ä–æ–µ–∑–∂–µ–π —á–∞—Å—Ç–∏, –æ—Å–≤–µ—â–µ–Ω–∏—è, —É–±–æ—Ä–∫–µ —Ç–µ—Ä—Ä–∏—Ç–æ—Ä–∏–∏ –∏ –¥—Ä—É–≥–∏–º –≤–æ–ø—Ä–æ—Å–∞–º.',\r\n            2: `–ß—Ç–æ–±—ã –æ—Ç–ø—Ä–∞–≤–∏—Ç—å —Å–æ–æ–±—â–µ–Ω–∏–µ, –≤ —Ä–∞–∑–¥–µ–ª–µ ¬´–°–æ–æ–±—â–µ–Ω–∏—è¬ª –Ω–∞ —Å—Ç–∞—Ä—Ç–æ–≤–æ–π —Å—Ç—Ä–∞–Ω–∏—Ü–µ –ü–æ—Ä—Ç–∞–ª–∞ –Ω–∞–∂–º–∏—Ç–µ –∫–Ω–æ–ø–∫—É ¬´–°–æ–æ–±—â–∏—Ç—å –æ –ø—Ä–æ–±–ª–µ–º–µ¬ª. –ó–∞–ø–æ–ª–Ω–∏—Ç–µ –Ω–µ–æ–±—Ö–æ–¥–∏–º—É—é –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ –≤–∞—à–µ–π –ø—Ä–æ–±–ª–µ–º–µ:\r\n                ‚Ä¢\t–≤—ã–±–µ—Ä–∏—Ç–µ –ø–æ–¥—Ö–æ–¥—è—â—É—é –∫–∞—Ç–µ–≥–æ—Ä–∏—é –∏, –ø–æ –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ—Å—Ç–∏, –ø–æ–¥–∫–∞—Ç–µ–≥–æ—Ä–∏—é —Å–æ–æ–±—â–µ–Ω–∏—è;\r\n                ‚Ä¢\t—É–∫–∞–∂–∏—Ç–µ –∞–¥—Ä–µ—Å –Ω–∞ –∫–∞—Ä—Ç–µ –∏–ª–∏ –≤–≤–µ–¥–∏—Ç–µ –µ–≥–æ –≤—Ä—É—á–Ω—É—é;\r\n                ‚Ä¢\t–∫—Ä–∞—Ç–∫–æ –æ–ø–∏—à–∏—Ç–µ —Å—É—Ç—å –ø—Ä–æ–±–ª–µ–º—ã, —Å—Ç–∞—Ä–∞–π—Ç–µ—Å—å —É–∫–∞–∑—ã–≤–∞—Ç—å —Ç–æ–ª—å–∫–æ —Ñ–∞–∫—Ç—ã;\r\n                ‚Ä¢\t–ø—Ä–∏–∫—Ä–µ–ø–∏—Ç–µ —Ñ–æ—Ç–æ–≥—Ä–∞—Ñ–∏–∏, –∏–ª–ª—é—Å—Ç—Ä–∏—Ä—É—é—â–∏–µ –ø—Ä–æ–±–ª–µ–º—É;\r\n                ‚Ä¢\t–≤—ã–±–µ—Ä–∏—Ç–µ –æ–¥–Ω—É –∏–∑ –æ–ø—Ü–∏–π –ø—É–±–ª–∏—á–Ω–æ–π (–æ–±—Ä–∞—â–µ–Ω–∏–µ –±—É–¥–µ—Ç –≤–∏–¥–Ω–æ –¥—Ä—É–≥–∏–º –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è–º –ü–æ—Ä—Ç–∞–ª–∞) –∏–ª–∏ –Ω–µ–ø—É–±–ª–∏—á–Ω–æ–π –ø–æ–¥–∞—á–∏ —Å–æ–æ–±—â–µ–Ω–∏—è;\r\n                ‚Ä¢\t–Ω–∞–∂–º–∏—Ç–µ –Ω–∞ –∫–Ω–æ–ø–∫—É ¬´–û—Ç–ø—Ä–∞–≤–∏—Ç—å —Å–æ–æ–±—â–µ–Ω–∏–µ¬ª.`,\r\n            3: '–í —Ç–µ—á–µ–Ω–∏–µ –¥–≤—É—Ö –¥–Ω–µ–π –æ–ø–µ—Ä–∞—Ç–æ—Ä –ü–æ—Ä—Ç–∞–ª–∞ –ø—Ä–æ–≤–æ–¥–∏—Ç –ø–µ—Ä–≤–∏—á–Ω—É—é –æ–±—Ä–∞–±–æ—Ç–∫—É —Å–æ–æ–±—â–µ–Ω–∏—è: –æ–Ω –æ–ø—Ä–µ–¥–µ–ª—è–µ—Ç –æ—Ç–≤–µ—Ç—Å—Ç–≤–µ–Ω–Ω–æ–≥–æ –∏—Å–ø–æ–ª–Ω–∏—Ç–µ–ª—è –∏ –Ω–∞–ø—Ä–∞–≤–ª—è–µ—Ç –∑–∞—è–≤–∫—É –µ–º—É –≤ —Ä–∞–±–æ—Ç—É. –ò—Å–ø–æ–ª–Ω–∏—Ç–µ–ª—å —Ñ–æ—Ä–º–∏—Ä—É–µ—Ç –æ—Ñ–∏—Ü–∏–∞–ª—å–Ω—ã–π –æ—Ç–≤–µ—Ç —Å –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏–µ–º, —á—Ç–æ –ø—Ä–æ–±–ª–µ–º–∞ —Ä–µ—à–µ–Ω–∞, –∏–ª–∏ —Å —É–∫–∞–∑–∞–Ω–∏–µ–º —Å—Ä–æ–∫–æ–≤, –∫–æ–≥–¥–∞ –æ–Ω–∞ –±—É–¥–µ—Ç —Ä–µ—à–µ–Ω–∞. –ú–∞–∫—Å–∏–º–∞–ª—å–Ω—ã–π —Å—Ä–æ–∫ –æ—Ç –ø–æ–¥–∞—á–∏ —Å–æ–æ–±—â–µ–Ω–∏—è –∂–∏—Ç–µ–ª–µ–º —á–µ—Ä–µ–∑ –ø–æ—Ä—Ç–∞–ª –¥–æ –ø–æ–ª—É—á–µ–Ω–∏—è –æ—Ç–≤–µ—Ç–∞ –æ—Ç –∏—Å–ø–æ–ª–Ω–∏—Ç–µ–ª—è ‚Äì 10 —Ä–∞–±–æ—á–∏—Ö –¥–Ω–µ–π.',\r\n            4: '–û—Ç—Å–ª–µ–∂–∏–≤–∞—Ç—å, –∫–∞–∫ —Ä–µ—à–∞–µ—Ç—Å—è –∑–∞—è–≤–ª–µ–Ω–Ω–∞—è –ø—Ä–æ–±–ª–µ–º–∞, –∏ –ø—Ä–æ—Å–º–∞—Ç—Ä–∏–≤–∞—Ç—å –æ—Ç–≤–µ—Ç—ã –∏—Å–ø–æ–ª–Ω–∏—Ç–µ–ª–µ–π –≤—ã –º–æ–∂–µ—Ç–µ –∏–ª–∏ –≤ –ª–∏—á–Ω–æ–º –∫–∞–±–∏–Ω–µ—Ç–µ, –∏–ª–∏ –ø–æ–ª—É—á–∞—è —É–≤–µ–¥–æ–º–ª–µ–Ω–∏—è –Ω–∞ —ç–ª–µ–∫—Ç—Ä–æ–Ω–Ω—É—é –ø–æ—á—Ç—É, —É–∫–∞–∑–∞–Ω–Ω—É—é –ø—Ä–∏ —Ä–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏–∏ –Ω–∞ –ø–æ—Ä—Ç–∞–ª–µ.',\r\n            5: '–ó–∞—è–≤–∫–∏, —Å–æ—Å—Ç–∞–≤–ª–µ–Ω–Ω—ã–µ –Ω–µ –≤ —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤–∏–∏ —Å ¬´–†–µ–≥–ª–∞–º–µ–Ω—Ç–æ–º –ø—Ä–∏—ë–º–∞ –∏ –æ–±—Ä–∞–±–æ—Ç–∫–∏ —Å–æ–æ–±—â–µ–Ω–∏–π¬ª, –≤–æ–∑–≤—Ä–∞—â–∞—é—Ç—Å—è –≤ –ª–∏—á–Ω—ã–π –∫–∞–±–∏–Ω–µ—Ç –¥–ª—è –≤–Ω–µ—Å–µ–Ω–∏—è –∏–∑–º–µ–Ω–µ–Ω–∏–π. –¢–∞–∫–∂–µ, —Å—Ç–æ–∏—Ç –ø—Ä–æ–≤–µ—Ä–∏—Ç—å, –ø–æ–¥—Ö–æ–¥–∏—Ç –ª–∏ –≤—ã–±—Ä–∞–Ω–Ω–∞—è –∫–∞—Ç–µ–≥–æ—Ä–∏—è —Ç–∏–ø—É –ø—Ä–æ–±–ª–µ–º—ã –∏ –≤—ã–±—Ä–∞—Ç—å –±–æ–ª–µ–µ –ø–æ–¥—Ö–æ–¥—è—â—É—é.',\r\n            6: '–ü–æ–ª—É—á–∏–≤ –æ—Ç–≤–µ—Ç –æ—Ç –∏—Å–ø–æ–ª–Ω–∏—Ç–µ–ª—è, –≤—ã –º–æ–∂–µ—Ç–µ –æ—Ü–µ–Ω–∏—Ç—å –∫–∞—á–µ—Å—Ç–≤–æ —Ä–µ—à–µ–Ω–∏—è –ø—Ä–æ–±–ª–µ–º—ã. –ï—Å–ª–∏ –æ–Ω–æ –≤–∞—Å –Ω–µ —É—Å—Ç—Ä–∞–∏–≤–∞–µ—Ç –∏–ª–∏ –ø—Ä–æ–±–ª–µ–º–∞ –Ω–µ —É—Å—Ç—Ä–∞–Ω–µ–Ω–∞, –∑–∞—è–≤–∫–∞ —Å–Ω–æ–≤–∞ –æ—Ç–ø—Ä–∞–≤–∏—Ç—Å—è –Ω–∞ –¥–æ—Ä–∞–±–æ—Ç–∫—É –∏—Å–ø–æ–ª–Ω–∏—Ç–µ–ª—é. –ï—Å–ª–∏ –≤—ã –¥–æ–≤–æ–ª—å–Ω—ã –∫–∞—á–µ—Å—Ç–≤–æ–º —Ä–µ—à–µ–Ω–∏—è, –æ–±—è–∑–∞—Ç–µ–ª—å–Ω–æ –æ—Ç–º–µ—Ç—å—Ç–µ —ç—Ç–æ –≤ –ª–∏—á–Ω–æ–º –∫–∞–±–∏–Ω–µ—Ç–µ.',\r\n        };\r\n        \r\n        const responseText = responses[id];\r\n        const widgetName = 'complaintsConversationReturn';\r\n\r\n        this.handleQuestion(responseText, widgetName);\r\n    }  \r\n\r\n    // HANDLE VOTINGS QUESTIONS\r\n\r\n    handleVotingsQuestionList = () => {\r\n        const responseTex